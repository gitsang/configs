{
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "diagnostic": {
    "displayByAle": true,
    "enable": true
  },
  "[py][python][go][golang][rust][lua][js][javascript][markdown]": {
    "coc.preferences.formatOnSave": true,
    "coc.preferences.formatOnType": true
  },
  "[c]": {
    "coc.preferences.formatOnSave": false,
    "coc.preferences.formatOnType": true
  },
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "go.goplsEnv": {
    "GOFLAGS": "-tags=cff"
  },
  "vimlsp.diagnostic.enable": true,
  "vetur.trace.server": "verbose",
  "prettier.trailingComma": "es5",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "snippets.ultisnips.pythonPrompt": false,
  "pyright.enable": true,
  "#clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd"
}
